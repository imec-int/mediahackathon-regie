@import common

body
	overflow hidden

iframe
	width 100%
	height 100%
	min-height 100%
	border 0px
	padding 0px
	margin 0px

#overlay
	position fixed
	background-position top center
	background-repeat no-repeat
	background-size contain
	width 100%
	height 100%
	min-height 100%
	border 0px
	padding 0px
	margin 0px
	z-index 10

.overlay-mediagoo
	background-image url("/images/overlay/mediagoo.png")
	background-color #6A6D6D

.overlay-hexamusic
	background-image url("/images/overlay/hexamusic.png")
	background-color #505419

.overlay-epleptic
	background-image url("/images/overlay/epleptic.png")
	background-color #505419

.overlay-paleEyes
	background-image url("/images/overlay/paleEyes.png")
	background-color #FFF